    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@storybook/addon-actions": "^6.2.1",
    "@storybook/addon-essentials": "^6.2.1",
    "@storybook/addon-links": "^6.2.1",
    "@storybook/react": "^6.2.1",
    "babel-loader": "^8.2.2",
    "prettier": "2.2.1"
  },
  "repository": {
    "type": "git",
